{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "API em NodeJS com Clean Architecture e TDD",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent --colors --noStackTrace",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "npm test -- --watch -c jest-integration.config.js",
    "test:staged": "npm test -- --passWithNoTests --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cams7-tests/clean-node-api.git"
  },
  "keywords": [
    "nodejs",
    "clean",
    "tdd"
  ],
  "author": "César A. Magalhães",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cams7-tests/clean-node-api/issues"
  },
  "homepage": "https://github.com/cams7-tests/clean-node-api#readme",
  "devDependencies": {
    "husky": "^4.3.8",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "validator": "^13.7.0"
  }
}
